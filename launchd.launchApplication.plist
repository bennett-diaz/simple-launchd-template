<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>launchd.launchApplication</string>
    
    <key>RunAtLoad</key>
    <true/>
   
   <key>ProgramArguments</key>
    <array>
        <!-- A command to launch the Calculator application -->
        <string>open</string>
        <string>-g</string>
        <string>-a</string>
        <string>/System/Applications/Calculator.app</string>
    </array>
    
    <key>StartInterval</key>
    <integer>10</integer>
  </dict>
</plist>